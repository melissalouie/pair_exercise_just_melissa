<div class="container">
  <div class="page-header">
    <h1><%= @location.city %></h1>
  </div>
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>City</th>
        <th>Street</th>
        <th>State</th>
        <th>Zip Code</th>
        <th>Country</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @location.city %></td>
        <td><%= @location.street_name %></td>
        <td><%= @location.state %></td>
        <td><%= @location.zip_code %></td>
        <td><%= @location.country %></td>
      </tr>
    </tbody>
  </table>
</div>
